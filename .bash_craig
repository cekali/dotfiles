alias test='echo hello'

alias git_find='find ~/ -name .git -type d -exec dirname {} \;'
alias bash_reload='source ~/.bash_profile'
alias wiki='vi ~/vimwiki/index.wiki'

function lazygit() {
    git add .
    git commit -a -m "$1"
    git push
}

function timestamp() {
    date
}

alias lazy_wiki='lazygit timestamp'

